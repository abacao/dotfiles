# auto-complete
if [ -f ~/.git-completion.bash ]; then
    . ~/.git-completion.bash
fi

if [ -f ~/gits/dotfiles/.bashrc.d/git-completion.bash ]; then
    . ~/gits/dotfiles/.bashrc.d/git-completion.bash
fi

#eval "$(direnv hook bash)"

complete -C '/usr/bin/aws_completer' aws

echo "Loaded autocompletion"
